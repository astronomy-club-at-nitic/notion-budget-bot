{
  "name": "notion-budget-bot",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "devDependencies": {
    "eslint": "8.47.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "9.0.0",
    "husky": "9.0.6",
    "lint-staged": "15.0.0",
    "prettier": "3.0.2",
    "scaffdog": "3.0.0",
    "turbo": "1.10.12"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@8.15.6",
  "volta": {
    "node": "18.20.1"
  }
}
