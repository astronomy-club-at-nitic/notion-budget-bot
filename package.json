{
  "name": "notion-budget-bot",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "devDependencies": {
    "eslint": "10.0.2",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.0.6",
    "lint-staged": "16.3.0",
    "prettier": "3.0.2",
    "scaffdog": "4.1.0",
    "turbo": "2.8.12"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@10.30.3",
  "volta": {
    "node": "18.20.8"
  }
}
