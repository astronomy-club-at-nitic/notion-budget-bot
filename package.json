    "lint-staged": "15.5.2",
    "prettier": "3.8.1",
    "scaffdog": "3.0.0",
    "turbo": "1.13.4"
  },
  "engines": {
    "node": ">=18.0.0 <19",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@10.30.3",
  "volta": {
    "node": "18.20.8"
  }
